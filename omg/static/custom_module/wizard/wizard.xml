<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <record id="custom_wizard_view_form" model="ir.ui.view">
        <field name="name">custom.wizard.form</field>
        <field name="model">custom.wizard</field>
        <field name="arch" type="xml">
            <form>
                <!-- <field name="custom_field" invisible="1"/> -->
                <group>
                    <group>
                        <field name="new_model_id"
                            required="1"
                            readonly="1"
                            widget="many2one"
                            options="{'no_create': True, 'no_open': True}"/>
                    </group>
                    <group>
                        <field name="is_checked"
                            widget="boolean_toggle"/>
                    </group>
                </group>
                <notebook>
                <page string="Partners">
                    <field name="partner_ids"
                        nolabel="1"
                        widget="many2many_tags"/>
                </page>
                </notebook>
                <footer>
                    <button name="validate" type="object" string="Validate" class="btn-primary"
                        attrs="{'invisible': [('is_checked', '=', False)]}"/>
                    <button string="Discard" special="cancel" class="btn-secondary"/>
                </footer>
            </form>
        </field>
    </record>
</odoo>
